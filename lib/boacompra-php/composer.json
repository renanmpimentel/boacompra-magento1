{
    "name": "uol/boacompra-php",
    "description": "Biblioteca de integração com o BoaCompra",
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.6.4",
        "ext-curl": "*",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "symfony/debug": "^3.3",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Uol\\BoaCompra\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uol\\BoaCompra\\Acceptance\\": "tests/acceptance/features/context/",
            "Uol\\BoaCompra\\Unit\\": "tests/unit/"
        }
    }
}
